<script>
    const modalCheckCourse = new tingle.modal({
        footer: true,
        stickyFooter: true
    })

    modalCheckCourse.addFooterBtn('Check Out', 'button -md -purple-1 text-white tingle-btn--pull-left', async function () {
        const items = []
        const courseItemClass = [...document.getElementsByClassName("course-item")]
        const courseItemBuy = courseItemClass.forEach(item => items.push({ courseID: item.getAttribute("courseID") }))
        const email = document.getElementById("email").value
        const FooterBtnID = document.getElementById("FooterBtnID")
        // FooterBtnID.classList.add('')
        FooterBtnID.classList.toggle("-single-icon");
        FooterBtnID.classList.toggle("-purple-1");
        FooterBtnID.classList.toggle("-purple-3");
        FooterBtnID.setAttribute('disabled', '');
        FooterBtnID.innerHTML = `<svg version="1.1" id="L9" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
        viewBox="0 0 100 100" enable-background="new 0 0 0 0" xml:space="preserve">
            <path fill="#fff" d="M73,50c0-12.7-10.3-23-23-23S27,37.3,27,50 M30.9,50c0-10.5,8.5-19.1,19.1-19.1S69.1,39.5,69.1,50">
            <animateTransform 
                attributeName="transform" 
                attributeType="XML" 
                type="rotate"
                dur="1s" 
                from="0 50 50"
                to="360 50 50" 
                repeatCount="indefinite" />
        </path>
        </svg> Check Out`
        try {
            toastr.success('Creating an order, please wait')

            const createOrder = await axios.post('/order/createvnapi', {
                email, items
            })
            FooterBtnID.classList.toggle("-single-icon");
            FooterBtnID.classList.toggle("-purple-1");
            FooterBtnID.classList.toggle("-purple-3");
            FooterBtnID.removeAttribute('disabled', '');
            FooterBtnID.innerHTML = 'Check Out'
            const itemsForGtag = createOrder.data.orderItem.map(item => {
                return {
                    item_id: item.id,
                    item_name: item.name,
                    price: item.priceus / 100,
                    quantity: 1
                }
            })
            const itemsForTikTok = createOrder.data.orderItem.map(item => {
                return {
                    content_id: item.id,
                    content_name: item.name,
                    price: item.priceus / 100,
                    quantity: 1
                }
            })
            gtag("event", "purchase", {
                transaction_id: createOrder.data.order.uuid,
                value: createOrder.data.order.priceus / 100,
                currency: "USD",
                items: itemsForGtag
            });
            fbq('track', 'Purchase', { currency: "USD", value: createOrder.data.order.priceus / 100 });
            ttq.track('PlaceAnOrder', {
                content_type: 'product',
                currency: "USD",
                value: createOrder.data.order.priceus / 100,
                contents: itemsForTikTok,

            })
            window.location.href = `/order/${createOrder.data.order.uuid}`
        } catch (error) {
            for (let e of error.response.data.error) {
                toastr.error(e.msg)
            }
        }

    });
    // modalCheckCourse.addFooterBtn('Huỷ', 'button -md -purple-3 text-purple-1 tingle-btn--pull-right', function () {
    //     modalCheckCourse.close();
    // });
    const handle_checkcourse = document.getElementById("handle_checkcourse");
    handle_checkcourse.addEventListener("submit", async (event) => {
        const FooterBtnID = document.getElementById("FooterBtnID")
        FooterBtnID.classList.toggle('d-none')
        try {
            event.preventDefault()
            const uri = document.getElementById("links").value
            const email = document.getElementById("email").value

            let links = Array.from(uri.match(/[(http(s)?):\/\/(www\.)?a-zA-Z0-9@:%._\+~#=]{2,256}\.[a-z]{2,6}\b([-a-zA-Z0-9@:%_\+.~#?&//=]*)/ig))

            modalCheckCourse.open()
            modalCheckCourse.setContent('Fetching data...');
            const checkCourseRequest = await axios.post('/course/check', {
                email, links
            })
            var codeHtml = ``


            for (let course of checkCourseRequest.data) {
                if (!course.success) {
                    toastr.error(course.data.title ? course.data.title : course.data.name, course.messenger)
                    continue
                }

                const childHTML = `<div class="border-bottom-light course-item mt-20" courseID="${course.data.id}">
              <div  class="coursesCard -type-1 row y-gap-20 flex-row items-center">
                <div class="col-2 my-2">
                  <div class="coursesCard__image rounded-8 relative">
                    <img class="w-1/1 rounded-8" src="${course.data.image}" alt="image">
                    <div class="coursesCard__image_overlay rounded-8"></div>
                  </div>
                </div>

                <div class="col-8">
                  <div class="text-17 lh-15 fw-500 text-dark-1 mt-10">${course.data.name}</div>
                </div>
                
                <div class="col-2">
                  <div class="text-right md:text-left">
                    <div class="text-15 lh-1 fw-500 line-through">
                        ${course.data.originprice?.toLocaleString('en-US', {
                    style: 'currency',
                    currency: 'USD'
                })}
                                                </div>
                    <div class="text-17 lh-1 fw-500 text-dark-1 mt-10">${(course.data.priceus / 100).toLocaleString('en-US', {
                    style: 'currency',
                    currency: 'USD'
                })}</div>
                  </div>
                </div>
              </div>
            </div>`
                codeHtml += childHTML
            }

            modalCheckCourse.setContent(codeHtml);
            FooterBtnID.innerHTML = `Check Out`
            FooterBtnID.classList.toggle('d-none')
        } catch (error) {
            console.log(error)
            toastr.error('Something is wrong, please check again or contact the administrator')
        }


    })
</script>

</html>