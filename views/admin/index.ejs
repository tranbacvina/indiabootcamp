<body class="" style="background-color: rgb(245, 244, 248)">
    <div id="root">
        <div class="App">
            <div class="header">
                <%- include('./nav'); %>

            </div>
            <div class="container-fluid">
                <div class="container-fluid p-4">
                    <div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div
                                    class="border-bottom pb-4 mb-4 d-lg-flex justify-content-between align-items-center">
                                    <div class="mb-3 mb-lg-0">
                                        <h1 class="mb-0 h2 fw-bold">Dashboard</h1>
                                    </div>
                                    <div class="d-flex">
                                        <div class="input-group me-3  "><input class="form-control flatpickr-input"
                                                placeholder="Select Date" value="" type="date" readonly="readonly"><span
                                                class="input-group-text text-muted" id="basic-addon2"><i
                                                    class="fe fe-calendar"></i></span></div><a class="btn btn-primary"
                                            href="/dashboard/overview">Setting</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xl-3 col-lg-6 col-md-12 col-sm-12">
                                <div class="mb-4 card border-light">
                                    <div class="card-body">
                                        <div class="d-flex align-items-center justify-content-between mb-3 lh-1">
                                            <div><span class="fs-6 text-uppercase fw-semi-bold">Doanh thu</span>
                                            </div>
                                            <div><span class="fe fe-shopping-bag fs-3 text-primary"></span>
                                            </div>
                                        </div>
                                        <h2 class="fw-bold mb-1">
                                            <%= sales[0].sales %>
                                        </h2><span class="ms-1 fw-medium">Doanh thu tất cả</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl-3 col-lg-6 col-md-12 col-sm-12">
                                <div class="mb-4 card border-light">
                                    <div class="card-body">
                                        <div class="d-flex align-items-center justify-content-between mb-3 lh-1">
                                            <div><span class="fs-6 text-uppercase fw-semi-bold">Doanh thu hôm nay</span>
                                            </div>
                                            <div><span class="fe fe-shopping-bag fs-3 text-primary"></span></div>
                                        </div>
                                        <h2 class="fw-bold mb-1">
                                            <% if (salesToday[0].salesToday=0) { %>
                                                0
                                                <% } else { %>
                                                    <%= salesToday[0].salesToday.toLocaleString('it-IT', {style
                                                        : 'currency' , currency : 'VND' }); %>
                                                        <%}%>
                                        </h2><span class="ms-1 fw-medium">Doanh thu hôm nay</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl-3 col-lg-6 col-md-12 col-sm-12">
                                <div class="mb-4 card border-light">
                                    <div class="card-body">
                                        <div class="d-flex align-items-center justify-content-between mb-3 lh-1">
                                            <div><span class="fs-6 text-uppercase fw-semi-bold">Khoá học cần tải</span>
                                            </div>
                                            <div><span class="fe fe-book-open fs-3 text-primary"></span></div>
                                        </div>
                                        <h2 class="fw-bold mb-1">
                                            <%= tongKhoaHocCanTai[0].tongKhoaHocCanTai %>
                                        </h2><span class="text-success fw-semi-bold"><span class="ms-1 fw-medium">Số
                                                lượng khoá học cần tải</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl-3 col-lg-6 col-md-12 col-sm-12">
                                <div class="mb-4 card border-light">
                                    <div class="card-body">
                                        <div class="d-flex align-items-center justify-content-between mb-3 lh-1">
                                            <div><span class="fs-6 text-uppercase fw-semi-bold">Khoá học mới</span>
                                            </div>
                                            <div><span class="fe fe-user-check fs-3 text-primary"></span></div>
                                        </div>
                                        <h2 class="fw-bold mb-1">
                                            <%=newCourseToday[0].newCourseToday %>
                                        </h2><span class="text-success fw-semi-bold"><i
                                                class="fe fe-trending-up me-1"></i>+200</span><span
                                            class="ms-1 fw-medium">Khoá học mới khách get hôm nay</span>
                                    </div>
                                </div>
                            </div>
                        </div>
